{
    "componentName": "ProjectCard",
    "type": "organism",
    "description": "Komponent karty projektu z pe≈ÇnƒÖ funkcjonalno≈õciƒÖ - thumbnail, metadane, postƒôp, modu≈Çy",
    "props": {
        "project": {
            "type": "ProjectWithStats",
            "required": true,
            "description": "Obiekt projektu ze statystykami"
        },
        "onEdit": {
            "type": "function",
            "required": false,
            "description": "Funkcja callback do edycji projektu"
        },
        "onDelete": {
            "type": "function",
            "required": false,
            "description": "Funkcja callback do usuniƒôcia projektu"
        }
    },
    "structure": {
        "main": {
            "type": "Card",
            "props": {
                "hoverable": true,
                "className": "project-card",
                "onClick": "handleCardClick"
            },
            "children": {
                "cover": {
                    "type": "div",
                    "className": "project-thumbnail",
                    "children": [
                        {
                            "condition": "project.miniatura exists",
                            "type": "img",
                            "props": {
                                "alt": "project.name",
                                "src": "project.miniatura",
                                "style": {
                                    "width": "100%",
                                    "height": 200,
                                    "objectFit": "cover"
                                }
                            }
                        },
                        {
                            "condition": "project.miniatura is null",
                            "type": "div",
                            "props": {
                                "style": {
                                    "width": "100%",
                                    "height": 200,
                                    "background": "linear-gradient(135deg, #667eea 0%, #764ba2 100%)",
                                    "display": "flex",
                                    "alignItems": "center",
                                    "justifyContent": "center",
                                    "color": "white",
                                    "fontSize": "2rem"
                                }
                            },
                            "content": "üñºÔ∏è"
                        }
                    ]
                },
                "actions": [
                    {
                        "type": "Button",
                        "props": {
                            "type": "text",
                            "icon": "FileTextOutlined"
                        },
                        "content": "Edytuj",
                        "onClick": "onEdit callback"
                    },
                    {
                        "type": "Button",
                        "props": {
                            "type": "text",
                            "icon": "LinkOutlined"
                        },
                        "content": "3D Model",
                        "onClick": "navigate to 3D model"
                    }
                ],
                "content": {
                    "type": "div",
                    "className": "project-card-content",
                    "children": [
                        {
                            "type": "project-header",
                            "children": [
                                {
                                    "type": "project-title-section",
                                    "children": [
                                        {
                                            "type": "Title",
                                            "props": {
                                                "level": 4,
                                                "style": {
                                                    "margin": 0,
                                                    "marginBottom": 4
                                                }
                                            },
                                            "content": "project.name"
                                        },
                                        {
                                            "type": "Text",
                                            "props": {
                                                "type": "secondary",
                                                "style": {
                                                    "fontSize": "0.85rem"
                                                }
                                            },
                                            "content": "Nr: project.numer"
                                        }
                                    ]
                                },
                                {
                                    "type": "StatusBadge",
                                    "props": {
                                        "status": "project.status"
                                    }
                                }
                            ]
                        },
                        {
                            "type": "project-meta",
                            "props": {
                                "style": {
                                    "marginBottom": 12
                                }
                            },
                            "children": [
                                {
                                    "type": "Tag",
                                    "props": {
                                        "color": "getProjectTypeColor(project.typ)"
                                    },
                                    "content": "project.typ"
                                },
                                {
                                    "type": "Text",
                                    "props": {
                                        "type": "secondary",
                                        "style": {
                                            "fontSize": "0.85rem"
                                        }
                                    },
                                    "content": "üìç project.lokalizacja"
                                }
                            ]
                        },
                        {
                            "type": "project-info",
                            "props": {
                                "style": {
                                    "marginBottom": 12
                                }
                            },
                            "children": [
                                {
                                    "type": "div",
                                    "props": {
                                        "style": {
                                            "display": "flex",
                                            "alignItems": "center",
                                            "marginBottom": 4
                                        }
                                    },
                                    "children": [
                                        {
                                            "type": "UserOutlined",
                                            "props": {
                                                "style": {
                                                    "marginRight": 6,
                                                    "color": "#666"
                                                }
                                            }
                                        },
                                        {
                                            "type": "Text",
                                            "props": {
                                                "strong": true
                                            },
                                            "content": "Klient: project.client"
                                        }
                                    ]
                                },
                                {
                                    "type": "div",
                                    "props": {
                                        "style": {
                                            "display": "flex",
                                            "alignItems": "center"
                                        }
                                    },
                                    "children": [
                                        {
                                            "type": "CalendarOutlined",
                                            "props": {
                                                "style": {
                                                    "marginRight": 6,
                                                    "color": "#666"
                                                }
                                            }
                                        },
                                        {
                                            "type": "Text",
                                            "content": "Deadline: formatDate(project.deadline)"
                                        }
                                    ]
                                }
                            ]
                        },
                        {
                            "type": "project-progress",
                            "props": {
                                "style": {
                                    "marginBottom": 16
                                }
                            },
                            "children": [
                                {
                                    "type": "div",
                                    "props": {
                                        "style": {
                                            "display": "flex",
                                            "justifyContent": "space-between",
                                            "marginBottom": 4
                                        }
                                    },
                                    "children": [
                                        {
                                            "type": "Text",
                                            "props": {
                                                "strong": true
                                            },
                                            "content": "Postƒôp"
                                        },
                                        {
                                            "type": "Text",
                                            "content": "project.postep || project.progress || 0%"
                                        }
                                    ]
                                },
                                {
                                    "type": "Progress",
                                    "props": {
                                        "percent": "project.postep || project.progress || 0",
                                        "size": "small",
                                        "strokeColor": {
                                            "0%": "#108ee9",
                                            "100%": "#87d068"
                                        }
                                    }
                                }
                            ]
                        },
                        {
                            "type": "project-modules",
                            "children": [
                                {
                                    "type": "Text",
                                    "props": {
                                        "strong": true,
                                        "style": {
                                            "fontSize": "0.9rem",
                                            "marginBottom": 8,
                                            "display": "block"
                                        }
                                    },
                                    "content": "MODU≈ÅY PROJEKTU:"
                                },
                                {
                                    "condition": "project.modules && project.modules.length > 0",
                                    "type": "modules-list",
                                    "children": [
                                        {
                                            "type": "map",
                                            "source": "project.modules.slice(0, 3)",
                                            "itemComponent": {
                                                "type": "div",
                                                "className": "module-item",
                                                "props": {
                                                    "style": {
                                                        "fontSize": "0.85rem",
                                                        "marginBottom": 2
                                                    }
                                                },
                                                "children": [
                                                    {
                                                        "type": "Text",
                                                        "content": "- module.replace('_', ' ')"
                                                    }
                                                ]
                                            }
                                        },
                                        {
                                            "condition": "project.modules.length > 3",
                                            "type": "Text",
                                            "props": {
                                                "type": "secondary",
                                                "style": {
                                                    "fontSize": "0.8rem"
                                                }
                                            },
                                            "content": "+ (project.modules.length - 3) wiƒôcej..."
                                        }
                                    ]
                                },
                                {
                                    "condition": "project.modules is empty",
                                    "type": "Text",
                                    "props": {
                                        "type": "secondary",
                                        "style": {
                                            "fontSize": "0.85rem"
                                        }
                                    },
                                    "content": "Brak zdefiniowanych modu≈Ç√≥w"
                                },
                                {
                                    "type": "div",
                                    "props": {
                                        "style": {
                                            "marginTop": 8,
                                            "paddingTop": 8,
                                            "borderTop": "1px solid #f0f0f0"
                                        }
                                    },
                                    "children": [
                                        {
                                            "type": "Text",
                                            "props": {
                                                "type": "secondary",
                                                "style": {
                                                    "fontSize": "0.8rem"
                                                }
                                            },
                                            "content": "≈ÅƒÖcznie element√≥w: ",
                                            "children": [
                                                {
                                                    "type": "Text",
                                                    "props": {
                                                        "strong": true
                                                    },
                                                    "content": "project.tilesCount"
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ]
                        },
                        {
                            "condition": "project.manager exists",
                            "type": "project-manager",
                            "props": {
                                "style": {
                                    "marginTop": 12,
                                    "paddingTop": 12,
                                    "borderTop": "1px solid #f0f0f0"
                                }
                            },
                            "children": [
                                {
                                    "type": "Space",
                                    "children": [
                                        {
                                            "type": "Avatar",
                                            "props": {
                                                "size": "small",
                                                "icon": "UserOutlined"
                                            }
                                        },
                                        {
                                            "type": "Text",
                                            "props": {
                                                "type": "secondary",
                                                "style": {
                                                    "fontSize": "0.85rem"
                                                }
                                            },
                                            "content": "Manager: project.manager"
                                        }
                                    ]
                                }
                            ]
                        }
                    ]
                }
            }
        }
    },
    "dataModel": {
        "ProjectWithStats": {
            "id": "string",
            "name": "string",
            "numer": "string",
            "status": "string",
            "typ": "string",
            "lokalizacja": "string",
            "client": "string",
            "deadline": "string",
            "postep": "number",
            "progress": "number",
            "miniatura": "string | null",
            "modules": "string[]",
            "tilesCount": "number",
            "manager": "string | null"
        }
    },
    "functions": {
        "handleCardClick": {
            "description": "Nawiguje do strony szczeg√≥≈Ç√≥w projektu",
            "action": "navigate(`/projekt/${project.id}`)"
        },
        "formatDate": {
            "description": "Formatuje datƒô do polskiego formatu",
            "params": [
                "dateString: string"
            ],
            "return": "string",
            "action": "new Date(dateString).toLocaleDateString('pl-PL')"
        },
        "getProjectTypeColor": {
            "description": "Zwraca kolor dla typu projektu",
            "params": [
                "typ: string"
            ],
            "return": "string",
            "mapping": {
                "Targi": "blue",
                "Scenografia TV": "purple",
                "Muzeum": "green",
                "Wystawa": "orange",
                "Event": "red",
                "Inne": "default"
            }
        }
    },
    "styles": {
        "project-card": {
            "hoverable": true,
            "className": "project-card"
        },
        "project-thumbnail": {
            "width": "100%",
            "height": 200,
            "objectFit": "cover"
        },
        "gradient-fallback": {
            "background": "linear-gradient(135deg, #667eea 0%, #764ba2 100%)",
            "display": "flex",
            "alignItems": "center",
            "justifyContent": "center",
            "color": "white",
            "fontSize": "2rem"
        },
        "progress-colors": {
            "0%": "#108ee9",
            "100%": "#87d068"
        }
    },
    "dependencies": {
        "antd": [
            "Card",
            "Progress",
            "Tag",
            "Avatar",
            "Space",
            "Typography",
            "Button"
        ],
        "antd-icons": [
            "CalendarOutlined",
            "UserOutlined",
            "FileTextOutlined",
            "LinkOutlined"
        ],
        "react-router": [
            "useNavigate"
        ],
        "components": [
            "StatusBadge"
        ],
        "types": [
            "ProjectWithStats"
        ]
    },
    "usage": {
        "example": {
            "jsx": "<ProjectCard project={projectData} onEdit={handleEdit} />",
            "description": "Podstawowe u≈ºycie komponentu z danymi projektu"
        },
        "withCallbacks": {
            "jsx": "<ProjectCard project={project} onEdit={handleEdit} onDelete={handleDelete} />",
            "description": "U≈ºycie z funkcjami callback"
        }
    },
    "atomicDesign": {
        "level": "organism",
        "description": "Kompleksowy komponent ≈ÇƒÖczƒÖcy wiele moleku≈Ç i atom√≥w",
        "atoms": [
            "Text",
            "Title",
            "Button",
            "Avatar",
            "Tag",
            "Progress"
        ],
        "molecules": [
            "StatusBadge",
            "Space"
        ],
        "organisms": [
            "ProjectCard"
        ]
    }
}
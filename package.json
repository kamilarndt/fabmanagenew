    "clsx": "^2.0.0",
    "i18next": "^23.7.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.5.0",
    "react-router-dom": "^7.0.2",
    "sonner": "^1.3.1",
    "tailwind-merge": "^2.2.0"
